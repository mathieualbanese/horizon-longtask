{
    "name": "mathieualbanese/horizon-longtask",
    "description": "Laravel library for managing long tasks with horizon",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu Albanese",
            "email": "mathieu.albanese@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.0"
    },
    "require-dev": {
        
    },
    "autoload": {
        "psr-4": {
            "MathieuAlbanese\\HorizonLongTask\\": "src/"
        }
    },
    "extra": {
